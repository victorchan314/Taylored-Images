{
    "manifest_version": 2,

    "name": "Taylored Images",
    "description": "Replaces all images on a webpage with images of Taylor Swift.",
    "version": "1.0",

    "permissions": [
        "activeTab"
    ],

    "background": {
        "scripts": ["https://ajax.googleapis.com/ajax/libs/jquery/3.2.1/jquery.min.js", "background.js"],
        "persistent": false
    },

    "content_scripts": [
        {
            "matches": ["<all_urls>"],
            "js": ["replaceImages.js"]
        }
    ],

    "browser_action": {
        "default_title": "Taylored Images"
    }
}
